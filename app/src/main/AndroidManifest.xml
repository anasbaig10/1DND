        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".final_page" />
        <activity android:name=".quiz_page5" />
        <activity android:name=".quiz_page4" />
        <activity android:name=".quiz_page3" />
        <activity android:name=".quiz_page2" />
        <activity android:name=".quiz_page1" />
        <activity android:name=".crush_name" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>